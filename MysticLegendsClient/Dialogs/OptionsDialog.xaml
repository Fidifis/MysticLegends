<Window x:Class="MysticLegendsClient.Dialogs.OptionsDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MysticLegendsClient.Dialogs"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        ResizeMode="NoResize"
        Title="Menu" Height="300" Width="250">
    <Grid>
        <StackPanel Margin="10, 30, 10, 30">
            <Button Content="Switch character" Margin="10" Height="30" Click="SwitchChar_Click"/>
            <Button Content="Logout" Margin="10" Height="30" Click="Logout_Click"/>
        </StackPanel>
    </Grid>
</Window>
