<Window x:Class="MysticLegendsClient.TravelWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MysticLegendsClient"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="Traveling" Height="680" Width="1000"
        MinWidth="400" MinHeight="400"
        Loaded="Window_Loaded">
    <Grid>
        <Image Source="/images/World/World1.png" Stretch="UniformToFill" HorizontalAlignment="Center" VerticalAlignment="Center" />
        <ProgressBar x:Name="progressBar" VerticalAlignment="Bottom" HorizontalAlignment="Stretch" Height="35" Margin="50"/>
        <Label x:Name="timer" Content="3" VerticalAlignment="Bottom" HorizontalAlignment="Center" Height="35" Margin="50" FontSize="20"/>
    </Grid>
</Window>
