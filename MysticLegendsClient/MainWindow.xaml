<Window x:Class="MysticLegendsClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MysticLegendsClient"
        mc:Ignorable="d"
        Title="Mystic Legends"
        Height="200" Width="400"
        MinHeight="100" MinWidth="250"
        WindowStartupLocation="CenterScreen"
        Loaded="Window_Loaded">
    <Grid>
        <Label Content="Mystic Legends" FontSize="30" VerticalAlignment="Center" HorizontalAlignment="Center"/>
        <ProgressBar IsIndeterminate="True" VerticalAlignment="Bottom" Height="10" Margin="20,0,20,20"/>
    </Grid>
</Window>
